{
  "name": "thanks-to",
  "version": "1.1.2",
  "description": "Generate beautiful acknowledgments for your project's dependencies",
  "type": "module",
  "main": "./dist/index.js",
  "bin": "./cli.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "test": "npm run build && npx vitest run --reporter verbose --coverage",
    "format": "prettier --write **/*.{js,ts}",
    "format:check": "prettier --check **/*.{js,ts}",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "build": "npx tsup"
  },
  "files": [
    "dist/*",
    "cli.js",
    "CHANGELOG.md"
  ],
  "keywords": [
    "thanks to",
    "acknowledgments",
    "license",
    "source"
  ],
  "author": "Yuki_akai",
  "homepage": "https://github.com/yukiakai212/thanks-to",
  "engines": {
    "node": ">= 16"
  },
  "maintainers": [
    {
      "name": "Yuki_akai",
      "homepage": "https://github.com/yukiakai212"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yukiakai212/thanks-to.git"
  },
  "bugs": {
    "url": "https://github.com/yukiakai212/thanks-to/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@eslint/js": "^9.30.1",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv": "^17.0.1",
    "eslint": "^9.30.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@yukiakai/find-up": "^1.1.5",
    "@yukiakai/resolve-package": "^1.0.2",
    "commander": "^14.0.0",
    "glob": "^11.0.3",
    "json-2-csv": "^5.5.9",
    "lodash": "^4.17.21",
    "spdx-correct": "^3.2.0",
    "vanipath": "^1.0.5"
  }
}
